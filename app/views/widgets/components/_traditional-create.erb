<div id="traditionalRegistration" style="display: block;" class="janrain-capture-ui capture-ui-content" data-captureventadded="true">
            <div class="capture_header">
                <input type="button" name="backButton" value="" data-capturefield="backButton" class="capture_backButton" id="capture_traditionalRegistration_backButton">
                <h3>Almost Done!</h3>
            </div>
                <form method="POST" class="capture_form capture_registrationForm" action="http://localhost:5012/widget/traditional_register.jsonp" name="registrationForm" id="capture_traditionalRegistration_registrationForm">                

                    <div class="capture_form_item capture_email capture_form_item_traditionalRegistration_emailAddress" id="capture_traditionalRegistration_form_item_traditionalRegistration_emailAddress"><label for="capture_traditionalRegistration_traditionalRegistration_emailAddress">Email Address</label><input type="email" name="traditionalRegistration_emailAddress" value="" data-capturefield="traditionalRegistration_emailAddress" class="capture_traditionalRegistration_emailAddress" id="capture_traditionalRegistration_traditionalRegistration_emailAddress">
